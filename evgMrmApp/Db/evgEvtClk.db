record(ao, "EVG$(cardNum):RFref") {
	field( DTYP, "EVG Clk RFref")
	field( DESC, "EVG Clk RF Frequency")
	field( OUT, "#C$(cardNum) S @")
	field( UDF, "0")
	field( EGU, "MHz")
	field( HOPR, "1600")
	field( LOPR, "50")
	field( DRVH, "1600")
	field( DRVL, "50")
	field( PREC, "3")
}

