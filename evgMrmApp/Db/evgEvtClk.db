record(bo, "$(P):EvtClkSrc") {
	field( DTYP, "EVG Clk")
  	field( DESC, "EVG Evt Clk Source")
  	field( OUT , "#C$(cardNum) S @")
	field( PINI, "YES")
	field( VAL, "0")
	field( UDF, "0")
  	field( ZNAM, "FracSyn")
  	field( ONAM, "RF")
	info(autosaveFields_pass0, "VAL")
	field( FLNK, "$(P):EvtClkSpeed")
}

