record(bo, "$(P):$(N):EnaIrq") {
	field( DTYP, "EVG Input IRQ")
	field( DESC, "Enable/Disable EVG External Input IRQ")
	field( OUT, "#C$(cardNum) S$(inpNum) @$(inpType)")
	field( PINI, "YES")
	field( VAL , "0")
	field( UDF, "0")
	field( ZNAM, "Disabled")
  	field( ONAM, "Enabled")
	info(autosaveFields_pass0, "VAL")
}

