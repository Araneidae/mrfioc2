record(mbboDirect, "$(P):$(N):DbusMap") {
  	field( DTYP, "EVG Input DBus")
  	field( OUT , "#C$(cardNum) S$(inpNum) @$(inpType)")
  	field( NOBT, "8")
	field( UDF, "0")
	field( B0,"0")
	field( B1,"0")
	field( B2,"0")
	field( B3,"0")
	field( B4,"0")
	field( B5,"0")
	field( B6,"0")
	field( B7,"0")
}

