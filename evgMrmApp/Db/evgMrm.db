record(bo, "$(P):Enable") {
	field( DTYP, "EVG")
  	field( DESC, "EVG Master Enable")
  	field( OUT , "#C$(cardNum) S @")
	field( UDF, "0")
	field( PINI, "YES")
	field( VAL, "1")
  	field( ZNAM, "Disabled")
  	field( ONAM, "Enabled")
	info(autosaveFields_pass0, "VAL")
}

