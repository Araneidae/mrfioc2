record(longout, "EVG$(cardNum):loadSeq") {
	field( DTYP, "EVG LOAD SEQ")
	field( DESC, "EVG Load Sequence")
	field( OUT, "#C$(cardNum) S @")
	field( VAL , "0")
	field( UDF, "0")
}

