#Sequence Records
record(waveform, "EVG$(cardNum):Seq$(seqNum):timeStamp") {
	field( DTYP, "EVG SEQ TIME STAMP")
	field( DESC, "Set the event code in EVG sequence Ram")
	field( INP, "#C$(cardNum) S$(seqNum) @")
	field( NELM, "$(NELM)")
	field( FTVL, "LONG")
}

record(waveform, "EVG$(cardNum):Seq$(seqNum):eventCode") {
	field( DTYP, "EVG SEQ EVENT CODE")
	field( DESC, "Set the time stamp in EVG sequence Ram")
	field( INP, "#C$(cardNum) S$(seqNum) @")
	field( NELM, "$(NELM)")
	field( FTVL, "UCHAR")
}

record(mbbo, "EVG$(cardNum):Seq$(seqNum):runMode") {
  	field( DTYP, "EVG SEQ RUN MODE")
  	field( OUT , "#C$(cardNum) S$(seqNum) @")
	field( UDF, "0")
  	field( ZRST, "Single")
  	field( ONST, "Automatic")
	field( TWST, "Normal")
}


