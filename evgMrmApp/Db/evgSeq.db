record(waveform, "EVG$(cardNum):Seq$(seqNum):timeStamp:tick") {
	field( DTYP, "EVG SEQ TIME STAMP")
	field( DESC, "Set the event code in EVG sequence Ram")
	field( INP, "#C$(cardNum) S$(seqNum) @")
	field( NELM, "$(NELM)")
	field( FTVL, "LONG")
}

record(waveform, "EVG$(cardNum):Seq$(seqNum):timeStamp:sec") {
	field( DTYP, "EVG SEQ TIME STAMP FLT")
	field( DESC, "Set the event code in EVG sequence Ram")
	field( INP, "#C$(cardNum) S$(seqNum) @")
	field( NELM, "$(NELM)")
	field( FTVL, "DOUBLE")
}

record(waveform, "EVG$(cardNum):Seq$(seqNum):eventCode") {
	field( DTYP, "EVG SEQ EVENT CODE")
	field( DESC, "Set the time stamp in EVG sequence Ram")
	field( INP, "#C$(cardNum) S$(seqNum) @")
	field( NELM, "$(NELM)")
	field( FTVL, "UCHAR")
}

record(mbbo, "EVG$(cardNum):Seq$(seqNum):runMode") {
  	field( DTYP, "EVG SEQ RUN MODE")
  	field( OUT , "#C$(cardNum) S$(seqNum) @")
	field( UDF, "0")
  	field( ZRST, "Single")
  	field( ONST, "Automatic")
	field( TWST, "Normal")
}

record(mbbo, "EVG$(cardNum):Seq$(seqNum):trigSrc") {
  	field( DTYP, "EVG SEQ TRIG SRC")
  	field( OUT , "#C$(cardNum) S$(seqNum) @")
	field( UDF, "0")
  	field( ZRST, "Mxc0")
  	field( ONST, "Mxc1")
  	field( TWST, "Mxc2")
	field( THST, "Mxc3")
	field( FRST, "Mxc4")
	field( FVST, "Mxc5")
	field( SXST, "Mxc6")
	field( SVST, "Mxc7")
	field( EIST, "AC")
	field( NIST, "RAM0")
	field( TEST, "RAM1")
	field( ZRVL, 0)
  	field( ONVL, 1)
  	field( TWVL, 2)
	field( THVL, 3)
	field( FRVL, 4)
	field( FVVL, 5)
	field( SXVL, 6)
	field( SVVL, 7)
	field( EIVL, 16)
	field( NIVL, 17)
	field( TEVL, 18)
}