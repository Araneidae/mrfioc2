#Sequence Ram Support Records
record(bo, "EVG$(cardNum):Seq$(seqNum):load") {
	field( DTYP, "EVG LOAD SEQ")
	field( DESC, "Load EVG Sequence")
	field( OUT, "#C$(cardNum) S$(seqNum) @")
	field( VAL , "0")
	field( UDF, "0")
}

record(bo, "EVG$(cardNum):Seq$(seqNum):unload") {
