record( stringin, "$(P):$(N):err") {
	field( DTYP, "EVG SEQ ERR")
  	field( DESC, "EVG SEQ ERR")
  	field( INP , "#C$(cardNum) S$(seqNum) @")
	field( SCAN, "I/O Intr")
}

record(waveform, "$(P):$(N):timeStamp:tick") {
	field( DTYP, "EVG SEQ TIME STAMP TICK")
	field( DESC, "Set the event code in EVG sequence Ram")
	field( INP, "#C$(cardNum) S$(seqNum) @")
	field( NELM, "$(NELM)")
	field( FTVL, "LONG")
}

