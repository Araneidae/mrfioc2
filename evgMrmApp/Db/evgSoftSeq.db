#
# On boot Process load record if the sequence was perviously in LOADED state
#

record(calcout, "$(P):$(N):calcoutLoad") {
    field( CALC, "A")
    field( INPA, "$(P):$(N):loadStatus NPP NMS")
    field( OOPT, "When Non-zero")
    field( OUT,  "$(P):$(N):load PP")
}

#
#Process commit record if the sequence was perviously in COMMITTED state
#

record(calcout, "$(P):$(N):calcoutCommit") {
    field( CALC, "A")
    field( INPA, "$(P):$(N):commitStatus NPP NMS")
    field( INPB, "$(P):$(N):timestamp PP")
    field( OOPT, "When Non-zero")
    field( OUT,  "$(P):$(N):commit PP")
}

record(calcout, "$(P):$(N):calcoutEnable") {
    field( CALC, "A")
    field( INPA, "$(P):$(N):enaStatus NPP NMS")
    field( OOPT, "When Non-zero")
    field( OUT,  "$(P):$(N):enable PP")
}

record(fanout, "$(P):$(N):fo") {
    field( DESC, "Boot of sequence for sequencer")
    field( SELM, "All")
    field( PINI, "YES")
    field( LNK1, "$(P):$(N):timestampInpMode")
    field( LNK2, "$(P):$(N):eventCode")
    field( LNK3, "$(P):$(N):timestamp")
    field( LNK4, "$(P):$(N):runMode")
    field( LNK5, "$(P):$(N):trigSrc")
    field( LNK6, "$(P):$(N):fo2")
}

record(fanout, "$(P):$(N):fo2") {
    field( DESC, "Boot of sequence for sequencer")
    field( SELM, "All")
    field( LNK1, "$(P):$(N):calcoutLoad")
    field( LNK2, "$(P):$(N):calcoutCommit")
    field( LNK3, "$(P):$(N):calcoutEnable")
}

