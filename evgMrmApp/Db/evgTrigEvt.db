record(bo, "$(P):$(N):Ena") {
        field( DTYP, "EVG Trig Evt")
        field( DESC, "EVG Trigger Event Enable")
        field( OUT , "#C$(cardNum) S$(trigEvtNum) @")
        field( ZNAM, "Disabled")
        field( ONAM, "Enabled")
        info(autosaveFields_pass0, "VAL")
}

record(longout, "$(P):$(N):Code") {
        field( DTYP, "EVG Trig Evt")
        field( DESC, "EVG Trigger Event Code")
        field( OUT, "#C$(cardNum) S$(trigEvtNum) @")
        field(HIHI, "256")
        field(LOLO, "-1")
        field(HHSV, "MAJOR")
        field(LLSV, "MAJOR")
        info(autosaveFields_pass0, "VAL")
}

