
record(calc, "$(BR){}Ratio:10kto1-I") {
    field(INPA, "$(INJ){EVG-Mxc:2}Prescaler-SP") # 10 KHz divider
    field(INPB, "$(INJ){EVG-Mxc:6}Prescaler-SP") # 1 Hz divider
    field(CALC, "A/B")
    field(HIGH, "11000")
    field(LOW , "9000")
    field(HSV , "INVALID")
    field(LSV , "INVALID")
    field(FLNK, "$(BR){}Ratio:10kto1-I")
}

record(calc, "$(BR){}Ratio:10kto2-I") {
    field(INPA, "$(BR){}Ratio:10kto1-I NPP MS")
    field(CALC, "A/2")
    field(FLNK, "")
}

record(calc, "$(BR){}Ratio:10k-I") {
    field(DESC, "Ratio of 10KHz to rep rate")
    field(INPA, "$(BR){}Ratio:10kto1-I NPP MS")
    field(INPB, "$(BR){}Ratio:10kto2-I NPP MS")
    field(INPC, "$(BR){}Mode-Sel CP MS")
    field(CALC, "C==2?B:A")
}
