file evgSoftSeq.db {
pattern { P, cardNum, seqNum, NELM}
{ "$(S){$(D)-SoftSeq:0}", $(cardNum), 0, 2047 }
{ "$(S){$(D)-SoftSeq:1}", $(cardNum), 1, 2047 }
{ "$(S){$(D)-SoftSeq:2}", $(cardNum), 2, 2047 }
}

