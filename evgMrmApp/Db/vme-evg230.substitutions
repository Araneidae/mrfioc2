file evgOutput.db {
pattern { P, cardNum, outNum, outType }
{ "$(S){$(D)-Out-FP:0}",   $(cardNum), 0, "FP_Output" }
{ "$(S){$(D)-Out-FP:1}",   $(cardNum), 1, "FP_Output" }
{ "$(S){$(D)-Out-FP:2}",   $(cardNum), 2, "FP_Output" }
{ "$(S){$(D)-Out-FP:3}",   $(cardNum), 3, "FP_Output" }
}

