file evgAcTrig.db {
pattern {P, cardNum}
{"$(S){$(D)-AcTrig}", $(cardNum)}
}

file evgDbus.db {
pattern { P, cardNum, dbusBit }
{ "$(S){$(D)-Dbus:0}", $(cardNum), 0 }
{ "$(S){$(D)-Dbus:1}", $(cardNum), 1 }
{ "$(S){$(D)-Dbus:2}", $(cardNum), 2 }
{ "$(S){$(D)-Dbus:3}", $(cardNum), 3 }
{ "$(S){$(D)-Dbus:4}", $(cardNum), 4 }
{ "$(S){$(D)-Dbus:5}", $(cardNum), 5 }
{ "$(S){$(D)-Dbus:6}", $(cardNum), 6 }
{ "$(S){$(D)-Dbus:7}", $(cardNum), 7 }
}

file evgEvtClk.db {
pattern { P, cardNum}
{"$(S){$(D)}", $(cardNum)}
}

file evgInput.db {
pattern { P, cardNum, inpNum, inpType }
{ "$(S){$(D)-Inp-FP:0}",   $(cardNum),  0, "FP_Input" }
{ "$(S){$(D)-Inp-FP:1}",   $(cardNum),  1, "FP_Input" }
{ "$(S){$(D)-Inp-Univ:0}", $(cardNum),  0, "Univ_Input" }
{ "$(S){$(D)-Inp-Univ:1}", $(cardNum),  1, "Univ_Input" }
{ "$(S){$(D)-Inp-Univ:2}", $(cardNum),  2, "Univ_Input" }
{ "$(S){$(D)-Inp-Univ:3}", $(cardNum),  3, "Univ_Input" }
{ "$(S){$(D)-Inp-TB:0}",   $(cardNum),  0, "TB_Input" }
{ "$(S){$(D)-Inp-TB:1}",   $(cardNum),  1, "TB_Input" }
{ "$(S){$(D)-Inp-TB:2}",   $(cardNum),  2, "TB_Input" }
{ "$(S){$(D)-Inp-TB:3}",   $(cardNum),  3, "TB_Input" }
{ "$(S){$(D)-Inp-TB:4}",   $(cardNum),  4, "TB_Input" }
{ "$(S){$(D)-Inp-TB:5}",   $(cardNum),  5, "TB_Input" }
{ "$(S){$(D)-Inp-TB:6}",   $(cardNum),  6, "TB_Input" }
{ "$(S){$(D)-Inp-TB:7}",   $(cardNum),  7, "TB_Input" }
{ "$(S){$(D)-Inp-TB:8}",   $(cardNum),  8, "TB_Input" }
{ "$(S){$(D)-Inp-TB:9}",   $(cardNum),  9, "TB_Input" }
{ "$(S){$(D)-Inp-TB:10}",  $(cardNum), 10, "TB_Input" }
{ "$(S){$(D)-Inp-TB:11}",  $(cardNum), 11, "TB_Input" }
{ "$(S){$(D)-Inp-TB:12}",  $(cardNum), 12, "TB_Input" }
{ "$(S){$(D)-Inp-TB:13}",  $(cardNum), 13, "TB_Input" }
{ "$(S){$(D)-Inp-TB:14}",  $(cardNum), 14, "TB_Input" }
{ "$(S){$(D)-Inp-TB:15}",  $(cardNum), 15, "TB_Input" }
}

