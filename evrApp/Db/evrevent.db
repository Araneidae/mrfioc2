
# EVT  - Timing hardware event code
# ENUM - EPICS DB event number
record(event, "$(P)$(N)") {
  field(DTYP, "EVR")
  field(SCAN, "I/O Intr")
  field(INP , "#C$(C) S$(CODE) @")
  field(VAL , "$(EVNT)")
}

record(calc, "$(P)$(N):count") {
  field(SCAN, "Event")
  field(CALC, "A+1")
  field(INPA, "$(P)$(N):count NPP")
  field(EVNT, "$(EVNT)")
}
