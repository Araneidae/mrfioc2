file "evrbase.db"
{pattern
{P, OBJ, EVNT1HZ}
{$(P), EVR$(C), 1}
}

file "databuftx.db"
{pattern
{P, C, PROTO}
{$(P), $(C), 1}
}

file "databuftxCtrl.db"
{pattern
{P, C}
{$(P), $(C)}
}

file "mrmevrbufrx.db"
{pattern
{P, C, PROTO}
{$(P), $(C), 1}
}

file "evrmap.db"
{pattern
{P, C, M, func, EVT}
{$(P), $(C), "blink1", Blink, 15}
{$(P), $(C), "blink2", Blink, 0}
}

file "evrevent.db"
{pattern
{P, C, N, CODE, EVNT}
{$(P), $(C), "dbevt", 15, 15}
{$(P), $(C), "1hz", 0x7d, 1}
}

file "evrscale.db"
{pattern
{P, IDX, O, OBJ, MAX}
{$(P), 0, "scale$(IDX)", "EVR$(C):PS$(IDX)", "0xffff"}
{$(P), 1, "scale$(IDX)", "EVR$(C):PS$(IDX)", "0xffff"}
{$(P), 2, "scale$(IDX)", "EVR$(C):PS$(IDX)", "0xffff"}
}

file "mrmevrout.db"
{pattern
{P, O, OBJ, DESC}
{$(P), "fpout0:", "EVR$(C):FrontUnivOut0", "UNIV0"}
{$(P), "fpout1:", "EVR$(C):FrontUnivOut1", "UNIV1"}
{$(P), "fpout2:", "EVR$(C):FrontUnivOut2", "UNIV2"}
{$(P), "fpout3:", "EVR$(C):FrontUnivOut3", "UNIV3"}
}

# Pulse generators w/o a prescaler set NOPS=1
file "evrpulser.db"
{pattern
{P, PID, N, OBJ, DMAX, WMAX, PMAX, NOPS}
{$(P), 0, "pul$(PID):", "EVR$(C):Pul$(PID)",  "0xfffffff", "0xffff", "0xff", 0}
{$(P), 1, "pul$(PID):", "EVR$(C):Pul$(PID)",  "0xfffffff", "0xffff", "0xff", 0}
{$(P), 2, "pul$(PID):", "EVR$(C):Pul$(PID)",  "0xfffffff", "0xffff", "0xff", 0}
{$(P), 3, "pul$(PID):", "EVR$(C):Pul$(PID)",  "0xfffffff", "0xffff", "0xff", 0}
{$(P), 4, "pul$(PID):", "EVR$(C):Pul$(PID)",  "0xfffffff", "0xffff", "1", 1}
{$(P), 5, "pul$(PID):", "EVR$(C):Pul$(PID)",  "0xfffffff", "0xffff", "1", 1}
{$(P), 6, "pul$(PID):", "EVR$(C):Pul$(PID)",  "0xfffffff", "0xffff", "1", 1}
{$(P), 7, "pul$(PID):", "EVR$(C):Pul$(PID)",  "0xfffffff", "0xffff", "1", 1}
{$(P), 8, "pul$(PID):", "EVR$(C):Pul$(PID)",  "0xfffffff", "0xffff", "1", 1}
}

# Default to 3 possible trigger mappings per pulser
file "evrpulsermap.db"
{pattern
{P, C, PID, F, N, M, EVT}
{$(P), $(C), 0, Trig, "pul0:", "trig0", 0}
{$(P), $(C), 0, Trig, "pul0:", "trig1", 0}
{$(P), $(C), 0, Trig, "pul0:", "trig2", 0}
{$(P), $(C), 1, Trig, "pul1:", "trig0", 0}
{$(P), $(C), 1, Trig, "pul1:", "trig1", 0}
{$(P), $(C), 1, Trig, "pul1:", "trig2", 0}
{$(P), $(C), 2, Trig, "pul2:", "trig0", 0}
{$(P), $(C), 2, Trig, "pul2:", "trig1", 0}
{$(P), $(C), 2, Trig, "pul2:", "trig2", 0}
{$(P), $(C), 3, Trig, "pul3:", "trig0", 0}
{$(P), $(C), 3, Trig, "pul3:", "trig1", 0}
{$(P), $(C), 3, Trig, "pul3:", "trig2", 0}
{$(P), $(C), 4, Trig, "pul4:", "trig0", 0}
{$(P), $(C), 4, Trig, "pul4:", "trig1", 0}
{$(P), $(C), 4, Trig, "pul4:", "trig2", 0}
{$(P), $(C), 5, Trig, "pul5:", "trig0", 0}
{$(P), $(C), 5, Trig, "pul5:", "trig1", 0}
{$(P), $(C), 5, Trig, "pul5:", "trig2", 0}
{$(P), $(C), 6, Trig, "pul6:", "trig0", 0}
{$(P), $(C), 6, Trig, "pul6:", "trig1", 0}
{$(P), $(C), 6, Trig, "pul6:", "trig2", 0}
{$(P), $(C), 7, Trig, "pul7:", "trig0", 0}
{$(P), $(C), 7, Trig, "pul7:", "trig1", 0}
{$(P), $(C), 7, Trig, "pul7:", "trig2", 0}
{$(P), $(C), 8, Trig, "pul8:", "trig0", 0}
{$(P), $(C), 8, Trig, "pul8:", "trig1", 0}
{$(P), $(C), 8, Trig, "pul8:", "trig2", 0}
}

file "evrin.db"
{pattern
{P, I, OBJ, DESC}
{$(P), "in0:", "EVR$(C):FPIn0", "IN0 (TTL)"}
{$(P), "in1:", "EVR$(C):FPIn1", "IN1 (TTL)"}
}
