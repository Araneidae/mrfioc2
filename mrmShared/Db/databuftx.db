
record(waveform, "$(P)dbus:send:s8") {
  field(DESC, "Send Buffer")
  field(DTYP, "MRF Data Buf Tx")
  field(INP , "@C=$(C), Proto=$(PROTO), P=Data Tx")
  field(FTVL, "CHAR")
  field(NELM, "2046")
}

